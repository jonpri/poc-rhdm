#!groovy
@Library("utils") _
eapPipeline {
	// Atributo que permite identificar el proyecto del componente de software a liberar. Ej: Proyecto Suitability Servicios EAP ( obligatorio)
        projectName = ""
	// Atributo que identifica el artifactId asociado al modulo a liberar, no del pom parent. Ej: cl.banchile.suitability-ear (obligatorio)
        artifactId = ""
	// Atributo que identifica el tipo de extension del binario ej:ear, jar. (obligatorio)
        extension= ""
	// Atributo que permite identificar la instancia EAP en la cual se instalara.Ej:00 , 01, 02, 03, 04
	instancia =""
}
